const scrapeIt = require("scrape-it")


// 1
scrapeIt("https://www.flamengo.com.br/elencos/elenco-profissional/diego-alves-carreira", {
    nome: "h2.text-white" , 
    posicao: "li.text-white:nth-of-type(1)" ,
    foto: {selector: "img.d-none" , attr: "src"} }


).then(({ data, response }) => {
    console.log(`Status Code: ${response.statusCode}`)
    console.log(data)
})


